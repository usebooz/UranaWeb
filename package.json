{
  "name": "uranaweb",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Telegram Mini App for UranaBot - Fantasy Football Tournament Display",
  "homepage": "https://usebooz.github.io/UranaWeb",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "dev:https": "cross-env HTTPS=true vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 20 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "preview": "vite preview",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@telegram-apps/sdk-react": "^3.3.1",
    "@telegram-apps/telegram-ui": "^2.1.8",
    "@tonconnect/ui-react": "^2.1.0",
    "eruda": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.4",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
